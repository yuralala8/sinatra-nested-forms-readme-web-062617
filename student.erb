<h1>Student</h1>

<div class="student">
  <h3>Name: <%= @student.name %></h3><br>
  <h4>Grade: <%= @student.grade %></h4>
</div><br>

<h1>Classes</h1>
<% @classes.each do |class| %>
  <div class="class">
    <p>Name: <%= class.name %></p><br>
    <p>Topic: <%= class.topic %></p><br>
  </div><br>
<% end %>
